
<main id="main">

    <section class="section">
      <div class="container">
        <div class="row mb-4 align-items-center">
          <div class="col-md-18" data-aos="fade-up">
            <h2>My Project</h2>
            <p>{{desc}}</p>
          </div>
        </div>
      </div>

      <div class="site-section pb-0">
        <div class="container">
          <div class="row align-items-stretch">
            <div class="col-md-8" data-aos="fade-up">
  <ul class="image-list">
    <li><img src="{{img}}" alt="Image" class="img-fluid"></li>
    <li><img src="{{img1}}" class="img-fluid"></li>
    <li><img src="{{img2}}" class="img-fluid"></li>
    <li><img src="{{img3}}" class="img-fluid"></li>
  </ul>
</div>
            <div class="col-md-3 ml-auto" data-aos="fade-up" data-aos-delay="100">
              <div class="sticky-content">
                <h3 class="h3">{{project_title}}</h3>
                <p class="mb-4"><span class="text-muted">{{framework}}</span></p>

                <h4 class="h4 mb-3">What I did</h4>
                <ul class="list-unstyled list-line mb-5">
                  <li>{{skl1}}</li>
                  <li>{{skl2}}</li>
                   {{#if skl3}}
                    <li>{{skl3}}</li>
                  {{/if}}
                  {{#if skl4}}
                    <li>{{skl4}}</li>
                  {{/if}}
                  {{!-- tried to iterate all the tools in tool object inside the object to make it more dynamic to specific projects! --}}
                  {{!-- {{#each skills}}
                  <li>{{this.tools}}</li>
                  {{/each}}  --}}
                </ul>

                <p><a href="{{link}}" class="readmore">Demo / Github Repo</a></p>
              </div>
            </div>
          </div>
        </div>
    </section>

    

           
        </div>

      </div>
    </section><!-- End Testimonials Section -->

  </main><!-- End #main -->

